        extId: '运维测试2',
        highest: {},
        id: 2,
        nickName: '测试',
        type: 0,
        userName: 'test1',
      },
      {
        deptName: '运维测试',
        extId: '运维测试1',
        highest: {},
        id: 1,
        nickName: '超级管理员',
        type: 0,
        userName: 'superAdmin',
      },
    ],
    deptName: '运维测试',
    extId: '7',
    highest: {
      deptName: '运维测试',
      id: 1,
      extId: '运维测试1',
      type: 0,
      userName: 'superAdmin',
      nickName: '超级管理员',
    },
    id: 7,
    nickName: '运维测试',
    sort: 10,
    type: 1,
  },
]
let finalPath = ''
function getChildren(id) {
  var hasFound = false
  var result = ''
  var fn = function (data, path = '') {
    if (Array.isArray(data) && !hasFound) {
      // 判断是否是数组并且没有的情况下，
      data.forEach((item) => {
        if (item.id === id) {
          // 数据循环每个子项，并且判断子项下边是否有id值
          hasFound=true;
          finalPath = path
          result = item
        } else if (item.children) {
          path += `${item.id},`
          fn(item.children, path) // 递归调用下边的子项
        }
      })
    }
  }
  fn(data) // 调用一下
  return result
}

let result = getChildren(7)
console.log('儿子', result)
finalPath += `${result.id}`

finalPath = finalPath.split(',')
fatherId = +finalPath[finalPath.length - 2]
console.log('爹', getChildren(fatherId))
